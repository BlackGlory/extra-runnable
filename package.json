{
  "name": "@blackglory/consumer",
  "version": "3.0.1",
  "description": "",
  "keywords": [],
  "type": "module",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "files": [
    "src/**/*.js",
    "src/**/*.js.map",
    "src/**/*.d.ts"
  ],
  "bin": {
    "run-consumer-module": "src/cli.js"
  },
  "repository": "git@github.com:BlackGlory/consumer.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "sideEffects": [
    "src/cli.js"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --quiet src __tests__",
    "test": "vitest run",
    "dev": "ts-node -r tsconfig-paths/register src/cli.ts",
    "dev:debug": "node --inspect-brk node_modules/.bin/ts-node -r tsconfig-paths/register src/cli.ts",
    "prepublishOnly": "run-s clean prepare build",
    "clean": "rimraf 'src/**/*.js' 'src/**/*.js.map' 'src/**/*.d.ts'",
    "build": "tsc --project tsconfig.build.json",
    "smoke": "node src/cli.js --help",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s clean prepare lint build test smoke clean",
      "commit-msg": "commitlint --config commitlint.config.cjs --env HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.31.0",
    "@types/debug": "^4.1.12",
    "@types/log-process-errors": "^9.1.0",
    "@types/node": "22",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "cross-env": "^7.0.3",
    "debug": "^4.4.1",
    "eslint": "^9.31.0",
    "husky": "^4.3.8",
    "npm-run-all": "^4.1.5",
    "return-style": "^3.0.1",
    "rimraf": "^6.0.1",
    "standard-version": "^9.5.0",
    "ts-patch": "^3.3.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "typescript-transform-paths": "^3.5.5",
    "vite": "^7.0.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@blackglory/prelude": "^0.4.0",
    "@blackglory/structures": "^0.14.10",
    "@blackglory/wait-for": "^0.8.1",
    "@delight-rpc/child-process": "^0.6.2",
    "@delight-rpc/extra-websocket": "^0.7.0",
    "@delight-rpc/websocket": "^0.7.4",
    "@delight-rpc/worker-threads": "^6.0.1",
    "bufferutil": "^4.0.9",
    "commander": "^14.0.0",
    "delight-rpc": "^6.1.2",
    "extra-abort": "^0.4.0",
    "extra-filesystem": "^0.5.2",
    "extra-fsm": "^0.2.1",
    "extra-promise": "^7.0.0",
    "extra-prompts": "^0.3.0",
    "extra-proxy": "^0.4.1",
    "extra-runnable": "^5.0.2",
    "extra-timers": "^0.2.6",
    "extra-websocket": "^0.4.2",
    "hardware-concurrency": "^0.1.4",
    "iterable-operator": "^5.1.0",
    "nanoid": "^5.1.5",
    "tsconfig-paths": "^4.2.0",
    "utf-8-validate": "^6.0.5",
    "ws": "^8.18.3",
    "you-died": "^0.4.5"
  }
}
