{
  "name": "extra-runnable",
  "version": "7.0.0",
  "description": "The Runnable and Runner in JavaScript/Typescript.",
  "keywords": [],
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": true,
  "files": [
    "src",
    "lib"
  ],
  "repository": "git@github.com:BlackGlory/extra-runnable.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --quiet src __tests__",
    "test": "run-s clean test:build test:run clean",
    "test:build": "tsc --project tsconfig.test.json",
    "test:run": "vitest run",
    "prepublishOnly": "run-s prepare clean build",
    "clean": "rimraf --glob lib 'src/**/*.js' 'src/**/*.js.map' 'src/**/*.d.ts'",
    "build": "tsc --project tsconfig.build.json",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare clean lint build test",
      "commit-msg": "commitlint --config commitlint.config.cjs --env HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/js": "^10.0.1",
    "@types/node": "22",
    "eslint": "^10.0.0",
    "extra-abort": "^0.4.1",
    "extra-filesystem": "^0.6.1",
    "husky": "^4.3.8",
    "npm-run-all": "^4.1.5",
    "return-style": "^3.0.1",
    "rimraf": "^6.1.2",
    "standard-version": "^9.5.0",
    "ts-patch": "^3.3.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "typescript-transform-paths": "^3.5.6",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@blackglory/prelude": "^0.4.0",
    "@delight-rpc/child-process": "^0.6.4",
    "@delight-rpc/worker-threads": "^6.0.3",
    "delight-rpc": "^6.1.2",
    "extra-fsm": "^0.2.1",
    "extra-promise": "^7.0.1",
    "hotypes": "^0.6.5",
    "nanoid": "^5.1.6",
    "url-operator": "^0.3.2"
  }
}
